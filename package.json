{
  "name": "sch-boilerplate",
  "version": "1.0.0",
  "description": "sch-boilerplate",
  "main": "index.js",
  "scripts": {
    "api": "json-server --watch  src/mocks/db.json --routes src/mocks/db-routes.json --port 8000",
    "build": "webpack --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production --profile -p",
    "start": "webpack-dev-server --mode=development --history-api-fallback --hot --inline --port 9090 --content-base public/",
    "start:open": "webpack-dev-server --mode=development --history-api-fallback --hot --host 0.0.0.0 --inline --port 9090 --open --content-base public/",
    "serve:prod": "cross-env NODE_ENV=production webpack-dev-server --port 9090 --history-api-fallback --content-base build/",
    "test": "jest --config=./unit.json --coverage"
  },
